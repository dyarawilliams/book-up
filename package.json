{
  "name": "book-up",
  "version": "1.0.0",
  "description": "A simple Rest API creating a book directory.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "compile:sass": "sass --watch --no-source-map scss:public/css",
    "build:css": "sass scss:public/css",
    "build:postcss": "postcss public/css/styles.css -o public/css/styles.min.css",
    "build": "npm run build:css && npm run build:postcss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyarawilliams/book-up.git"
  },
  "keywords": [
    "REST",
    "API",
    "book",
    "directory"
  ],
  "author": "D'yara Williams",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dyarawilliams/book-up/issues"
  },
  "homepage": "https://github.com/dyarawilliams/book-up#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "autoprefixer": "^10.4.20",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.3",
    "compression": "^1.7.5",
    "connect-mongo": "^5.1.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "method-override": "^3.0.0",
    "mongodb": "^6.5.0",
    "mongoose": "^8.2.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "cssnano": "^7.0.7",
    "nodemon": "^3.1.0",
    "postcss": "^8.5.4",
    "postcss-cli": "^11.0.1",
    "sass": "^1.72.0"
  }
}
